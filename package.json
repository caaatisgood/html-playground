{
  "name": "html-playground",
  "version": "0.1.0",
  "description": "html-playground",
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev webpack-dev-server -d --progress --colors --port 8700",
    "dev:hbs": "cross-env webpack-dev-server -d --config webpack.config.hbs.js --progress --colors --port 8700",
    "build": "cross-env NODE_ENV=prod webpack --optimize-minimize --progress --colors",
    "build:hbs": "node ./src/hbs/index.js"
  },
  "author": "Robin",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^10.1.0",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  }
}
